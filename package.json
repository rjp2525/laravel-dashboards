{
    "name": "@rjp2525/laravel-dashboards",
    "version": "0.1.0",
    "description": "Vue 3 frontend for the Laravel Dashboards package",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": ["dist"],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./renderers/echarts": {
            "types": "./dist/renderers/echarts.d.ts",
            "import": "./dist/renderers/echarts.js"
        },
        "./renderers/apexcharts": {
            "types": "./dist/renderers/apexcharts.d.ts",
            "import": "./dist/renderers/apexcharts.js"
        },
        "./renderers/chartjs": {
            "types": "./dist/renderers/chartjs.d.ts",
            "import": "./dist/renderers/chartjs.js"
        },
        "./renderers/unovis": {
            "types": "./dist/renderers/unovis.d.ts",
            "import": "./dist/renderers/unovis.js"
        }
    },
    "scripts": {
        "build": "vite build",
        "dev": "vite build --watch",
        "typecheck": "vue-tsc --noEmit"
    },
    "peerDependencies": {
        "vue": "^3.4",
        "@inertiajs/vue3": "^2.0",
        "echarts": "^5.5",
        "vue-echarts": "^7.0",
        "apexcharts": "^3.0 || ^4.0",
        "vue3-apexcharts": "^1.5",
        "chart.js": "^4.0",
        "vue-chartjs": "^5.0",
        "vue-chrts": "^0.1",
        "laravel-echo": "^2.0"
    },
    "peerDependenciesMeta": {
        "echarts": { "optional": true },
        "vue-echarts": { "optional": true },
        "apexcharts": { "optional": true },
        "vue3-apexcharts": { "optional": true },
        "chart.js": { "optional": true },
        "vue-chartjs": { "optional": true },
        "vue-chrts": { "optional": true },
        "laravel-echo": { "optional": true }
    },
    "dependencies": {
        "gridstack": "^10.0"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^5.0",
        "typescript": "^5.5",
        "vite": "^6.0",
        "vite-plugin-dts": "^4.0",
        "vue": "^3.4",
        "vue-tsc": "^2.0",
        "@inertiajs/vue3": "^2.0",
        "echarts": "^5.5",
        "vue-echarts": "^7.0"
    }
}
